@model mvc.Models.tblemployee
@{
    ViewBag.Title = "insert";
}

<h2>insert</h2>


@using (Html.BeginForm())
{
    <div class=" form-group">
        @Html.TextBoxFor(obj => obj.firstname, new { @class = "form-control" })
    </div>
    <div class=" form-group">
        @Html.TextBoxFor(obj => obj.lastname, new { @class = "form-control" })
    </div>
    <div class=" form-group">
        @Html.TextBoxFor(obj => obj.address, new { @class = "form-control" })
    </div>
    <div class=" form-group">
        @Html.TextBoxFor(obj => obj.phone, new { @class = "form-control" })
    </div>
    <input type="submit" value="insert" name="name" class="btn btn-success" />

}






































